{
  "name": "@arborium/<%= grammar_id %>",
  "version": "<%= version %>",
  "description": "Syntax highlighting for <%= grammar_name %>, powered by WebAssembly",
  "type": "module",
  "main": "./grammar.js",
  "module": "./grammar.js",
  "exports": {
    ".": "./grammar.js",
    "./grammar.js": "./grammar.js",
    "./grammar_bg.wasm": "./grammar_bg.wasm"
  },
  "files": ["grammar.js", "grammar_bg.wasm"],
  "sideEffects": false,
  "keywords": [
    "arborium",
    "syntax-highlighting",
    "tree-sitter",
    "<%= grammar_id %>",
    "<%= grammar_name %>",
    "wasm"
  ],
  "author": "Amos Wenger <amos@bearcove.eu>",
  "license": "MIT OR Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bearcove/arborium"
  },
  "homepage": "https://github.com/bearcove/arborium",
  "bugs": {
    "url": "https://github.com/bearcove/arborium/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/fasterthanlime"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
